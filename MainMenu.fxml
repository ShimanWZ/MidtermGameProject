            <Button id="MainMenuBottons" layoutX="944.0" layoutY="100" mnemonicParsing="false" onAction="#startNewGame" prefHeight="75.0" prefWidth="371.0" stylesheets="@application.css" text="Start New Game" textAlignment="CENTER" textFill="WHITE">
